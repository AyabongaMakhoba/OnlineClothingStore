@model IEnumerable<OnlineWebApp.Models.AppModels.OrderList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DriverInfos.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderDetails.OrderDetail_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Staff.StaffName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DriverInfos.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDetails.OrderDetail_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Staff.StaffName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OrderList_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.OrderList_ID }) |
