@model IEnumerable<OnlineWebApp.Models.RecievedStock>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Items.Item_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Supplier.SupplierName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfStock)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.NewTotalStock)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.StockDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
	<td>
		@Html.DisplayFor(modelItem => item.Items.Item_Name)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.Supplier.SupplierName)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.NumberOfStock)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.NewTotalStock)
	</td>
	<td>
		@Html.DisplayFor(modelItem => item.StockDate)
	</td>
	<td>
		@Html.ActionLink("Edit", "Edit", new { id = item.StockID }) |
		@Html.ActionLink("Details", "Details", new { id = item.StockID }) |
		@Html.ActionLink("Delete", "Delete", new { id = item.StockID })
	</td>
</tr>
}

</table>
